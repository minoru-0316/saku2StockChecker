<h1>【マスタ】職業</h1>

<table>
  <thead>
    <tr>
      <th>職業id</th>
      <th>職業コード</th>
      <th>職業</th>
    </tr>
  </thead>

  <tbody>
    <% @jobs.each do |job| %>
    <tr>
      <td><%=job.id %></td>
      <td><%= job.job_code %></td>
      <td><%=  job.job_name %></td>
     </tr>
    <% end %>
  </tbody>
</table>